+
+    const blocksQuery = useMemoFirebase(() => (firestore && authUser) ? query(collection(firestore, 'blocks'), where('blockerId', '==', authUser.uid)) : null, [firestore, authUser]);
+    const { data: blocks } = useCollection<Block>(blocksQuery);
